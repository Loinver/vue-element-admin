    "lodash.find": "^4.6.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "6.1.0",
    "vue": "2.6.11",
    "vue-cropper": "^0.5.2",
    "vue-router": "3.1.6",
    "vuex": "3.1.3"
